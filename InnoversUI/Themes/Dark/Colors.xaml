<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--THEMES COLORS-->
    <Color x:Key="MenuColor" >#FF1C0337</Color>
    <Color x:Key="MenuSelectedItemColor">DarkCyan</Color>
    <Color x:Key="MenuBtnOverColor">#3A144A</Color>

    <Color x:Key="BackColor" >#FF37304C</Color>
    <Color x:Key="ForegroundColor" >#FFFFFF</Color>
    <Color x:Key="DataGridAlternateRowBackColor" >#666A5C90</Color>
    <Color x:Key="ScrollBarColor" >#FFB3A8A8</Color>
    <Color x:Key="ScrollBarMouseOverColor" >#FF8C8787</Color>
    <Color x:Key="ChartForegroundColor" >LightGray</Color>
    
    <Color x:Key="AntiDarkForegroundColor" >#FF1C0337</Color>
    <Color x:Key="AntiLightForegroundColor" >#FFFFFF</Color>
    <Color x:Key="AntiDarkBackColor" >Azure</Color>
    <Color x:Key="AntiLightBackColor" >#FF37304C</Color>
